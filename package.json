{
  "name": "hexlet-interview",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && npx tsc -p tsconfig.build.json && npm run build:client",
    "build:client": "webpack -p --env production",
    "watch:server": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "watch:client": "webpack --watch --display minimal",
    "start:dev": "concurrently npm:watch:*",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "NODE_ENV=production node dist/main.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --ext .ts,.js .",
    "lint:fix": "eslint --ext .ts,.js . --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --verbose=false",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "migration:run": "npm run typeorm migration:run",
    "migration:generate": "npm run typeorm migration:create -- -n",
    "schema:drop": "npm run typeorm schema:drop",
    "fixtures:load": "fixtures ./test/fixtures --config ./src/ormconfig.ts --require=ts-node/register --require=tsconfig-paths/register"
  },
  "dependencies": {
    "@babel/preset-env": "^7.6.3",
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/typeorm": "^6.1.3",
    "@types/helmet": "0.0.44",
    "@types/nodemailer": "^6.2.1",
    "@types/passport-github2": "^1.2.4",
    "autoprefixer": "^9.6.5",
    "babel-loader": "^8.0.6",
    "bcrypt": "^3.0.6",
    "bootstrap": "^4.3.1",
    "class-transformer": "0.2.3",
    "class-validator": "0.10.0",
    "css-loader": "^3.2.0",
    "date-fns": "^2.6.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-flash": "^0.0.2",
    "express-rate-limit": "^5.0.0",
    "express-session": "^1.16.2",
    "helmet": "^3.21.1",
    "i18n": "^0.8.3",
    "jquery": "^3.4.1",
    "jquery-ujs": "^1.2.2",
    "lodash": "^4.17.15",
    "memorystore": "^1.6.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nodemailer": "^6.3.0",
    "passport": "^0.4.0",
    "passport-github2": "^0.1.11",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "popper.js": "^1.16.0",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.4",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^6.3.3",
    "sass-loader": "^8.0.0",
    "sqlite3": "^4.1.0",
    "typeorm": "0.2.25",
    "typeorm-fixtures-cli": "^1.3.5",
    "typeorm-seeding": "^1.0.0-rc.3",
    "uuid": "^3.3.3",
    "webpack": "^4.41.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "4.16.1",
    "@types/express-flash": "0.0.1",
    "@types/express-session": "^1.15.14",
    "@types/faker": "^4.1.5",
    "@types/i18n": "^0.8.6",
    "@types/jest": "24.0.11",
    "@types/lodash": "^4.14.142",
    "@types/nock": "^11.1.0",
    "@types/node": "^12.7.11",
    "@types/passport-jwt": "^3.0.2",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "2.0.7",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "babel-eslint": "^10.0.3",
    "concurrently": "^5.0.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "^4.1.0",
    "jest": "24.7.1",
    "nock": "^11.3.5",
    "nodemailer-mock-transport": "1.3.0",
    "prettier": "1.17.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "^8.3.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "typescript": "3.4.3",
    "webpack-cli": "^3.3.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Hexlet/hexlet-interview.git"
  }
}
